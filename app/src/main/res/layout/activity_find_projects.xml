    <Button
        android:id="@+id/button19"
        android:layout_width="85dp"
        android:layout_height="34dp"
        android:background="@color/White"
        android:text="See more"
        android:textAllCaps="false"
        android:textColor="@color/Indigo"
        app:layout_constraintEnd_toEndOf="@+id/editText10"
        tools:layout_editor_absoluteY="256dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Communities in Schools of San Antonio"
        android:textColor="@color/Black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/editText11"
        tools:layout_editor_absoluteY="300dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Protect All"
        android:textColor="@color/Black"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/editText12"
        tools:layout_editor_absoluteY="435dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Michigan Medicine"
        android:textColor="@color/Black"
        android:textSize="18sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="181dp" />

    <Button
        android:id="@+id/button20"
        android:layout_width="85dp"
        android:layout_height="34dp"
